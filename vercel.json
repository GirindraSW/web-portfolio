{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "src/views/**"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(css|img|js|cv)/(.*)", "dest": "/src/assets/$1/$2" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ]
}
